@model MvcLabManager.Models.Computer

@{
    ViewData["Title"] = $"Computador {Model.Id}";
}
<h1>@ViewData["Title"]</h1>

<p><b>ID:</b> @Model.Id</p>
<p><b>RAM:</b> @Model.Ram</p>
<p><b>PROCESSOR:</b> @Model.Processor</p>
<a href="/Computer/Atualizar/@Model.Id" class="btn btn-outline-success">Atualizar</a>
<a href="/Computer/Delete/@Model.Id" class="btn btn-outline-danger">Excluir</a>
<a href="/Computer/Index" class="btn btn-outline-dark">Voltar</a>
