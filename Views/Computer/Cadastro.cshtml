@{
    ViewData["Title"] = "Cadastrar do Computador";
}

<h1>@ViewData["Title"]</h1><br>

<form action="/Computer/ValidarCadastro" method="post">
    <center>
        <table>
            <tr>
                <td><label for="id">Id:</label></td>
                <td><input type="number" name="id" id="id" required></td>
            </tr>
            <tr>
                <td><label for="ram">RAM:</label></td>
                <td><input type="text" name="ram" id="ram" required></td>
            </tr>
            <tr>
                <td><label for="processor">Processador:</label></td>
                <td><input type="text" name="processor" id="processor" required></td>
            </tr>
        </table><br>
        <input type="submit" class="btn btn-primary btn-sm btn-success" value="Cadastrar"><br>
    </center>
</form>

<a href="/Computer/Index" class="btn btn-outline-dark">Voltar</a><br><br>
